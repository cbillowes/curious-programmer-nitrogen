{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs",
            "debug": false
          }
        ],
        "@babel/preset-flow",
        "@babel/preset-react"
      ],
      "plugins": ["@babel/plugin-transform-modules-commonjs",
                  "@babel/plugin-syntax-dynamic-import",
                  "@babel/plugin-proposal-class-properties"]
    }
  }
}
